{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-src 'self' https://open.spotify.com https://*.spotify.com;"
        }
      ]
    }
  ]
}
